@model NeoWeb.Models.Candidate
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

@{
    ViewData["Title"] = Localizer["Governance"].Value;
}

@section Styles{
    <link rel="stylesheet" href="@Helper.ToCDN("/css/gov.min.css", true)" />
}
<div class="py-5">
    @Localizer["Governance"]
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <environment include="Development">
        <script src="/js/gov.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="@Helper.ToCDN("/js/gov.min.js", true)"></script>
    </environment>
}
