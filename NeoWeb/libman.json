{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/"
    },
    {
      "library": "tinymce@5.7.0",
      "destination": "wwwroot/lib/tinymce/"
    },
    {
      "library": "bootstrap-fileinput@5.1.5",
      "destination": "wwwroot/lib/bootstrap-fileinput/",
      "files": [
        "js/fileinput.min.js",
        "js/locales/zh.js",
        "js/locales/zh.min.js",
        "css/fileinput.min.css",
        "img/loading-sm.gif",
        "img/loading.gif"
      ]
    }
  ]
}
